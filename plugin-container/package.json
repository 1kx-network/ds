{
  "name": "loot-dungeon-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others -n nextjs,codegen npm:start:dev npm:watch:graphql",
    "build:contracts": "typechain --target ethers-v5 --out-dir ./src/services/contracts ../solidity/artifacts/contracts/CompetitionMinter.sol/CompetitionMinter.json ../solidity/artifacts/contracts/Dungeon.sol/Dungeon.json ../solidity/artifacts/contracts/Dungeon2.sol/Dungeon2.json ../solidity/artifacts/contracts/Dungeon3Minter.sol/Dungeon3Minter.json ../solidity/artifacts/contracts/Loot.sol/Loot.json ../solidity/artifacts/contracts/TemporalLoot.sol/TemporalLoot.json ../solidity/artifacts/contracts/LootComponents.sol/LootComponents.json ../solidity/artifacts/contracts/LootHelper.sol/LootHelper.json ../solidity/artifacts/contracts/Relic.sol/Relic.json ../solidity/artifacts/contracts/GenesisSeekerMinter.sol/GenesisSeekerMinter.json ../solidity/artifacts/contracts/Seeker.sol/Seeker.json",
    "build:graphql": "graphql-codegen --config codegen.yml",
    "build": "next build && next export",
    "start:dev": "next dev -p 3005",
    "start": "next start",
    "lint": "tsc --noEmit && eslint . --ext .ts --ext .tsx --quiet --fix",
    "watch:graphql": "graphql-codegen --config codegen.yml -w"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "6.2.9",
    "async": "^3.2.3",
    "bootstrap": "^4.6.0",
    "change-case": "^4.1.2",
    "css-in-js-media": "^1.0.15",
    "ethers": "^5.5.2",
    "graphql": "^16.3.0",
    "graphql-ws": "^5.6.4",
    "hash-sum": "^2.0.0",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.27",
    "next": "^11.1.4",
    "polished": "^4.1.1",
    "react": "17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-device-detect": "^2.1.2",
    "react-dom": "17.0.2",
    "react-unity-webgl": "^8.8.0",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@ethersproject/providers": "^5.5.3",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.1",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@graphql-eslint/eslint-plugin": "^3.9.1",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.0.0",
    "@types/async": "^3.2.12",
    "@types/hash-sum": "^1.0.0",
    "@types/node": "16.11.7",
    "@types/react": "^17.0.1",
    "@types/react-bootstrap": "^0.32.25",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "concurrently": "^7.0.0",
    "debug-provider": "file:../playmint/debug-provider",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "hygen": "^6.0.4",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "react-cookie-consent": "^7.2.1",
    "typechain": "^6.0.5",
    "typescript": "^4.3.5"
  }
}
