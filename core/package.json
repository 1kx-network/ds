{
  "name": "dawnseekers",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev -c vite.preview.config.ts",
    "preview": "vite build -c vite.preview.config.ts && vite preview -c vite.preview.config.ts",
    "build": "graphql-codegen && tsc --build --verbose",
    "build:watch": "tsc --build -w",
    "test": "vitest --run",
    "coverage": "vitest run --coverage"
  },
  "files": "./dist",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "peerDependencies": {
    "ethers": "^6.0.8",
    "quickjs-emscripten": "^0.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/client-preset": "^2.1.1",
    "@graphql-codegen/urql-introspection": "^2.2.1",
    "@graphql-eslint/eslint-plugin": "^3.16.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "react-hexgrid": "^2.0.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-graphql-codegen": "^3.1.0",
    "vitest": "^0.29.2"
  },
  "dependencies": {
    "@types/dompurify": "^2.4.0",
    "@uiw/react-textarea-code-editor": "^2.1.1",
    "@urql/core": "^3.2.2",
    "@urql/exchange-graphcache": "^5.2.0",
    "@urql/introspection": "^1.0.0",
    "dompurify": "^3.0.1",
    "graphql-ws": "^5.12.0"
  }
}
